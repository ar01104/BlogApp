<% if current_user %>
<p id="notice"><%= notice %></p>
<div id="post">


<p>
  <strong>User:</strong>
  <%= @post.user_id %>
</p>

<p>
  <strong>Title:</strong>
  <%= @post.title %>
</p>

<p>
  <strong>Description:</strong>
  <%= @post.description %>
</p>

<p>
  <strong>Posting date:</strong>
  <%= @post.posting_date %>
</p>

<%= link_to 'Edit', edit_post_path(@post) %> |
</div>
<% end %>
